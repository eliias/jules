  },
  "bugs": {
    "url": "https://github.com/eliias/jules/issues"
  },
  "dependencies": {
    "jquery": "^2.1.3",
    "lodash": "^3.3.0",
    "phaser": "^2.2.2"
  },
  "devDependencies": {
    "browserify": "^8.1.3",
    "browserify-shim": "^3.8.2",
    "uglify-js": "^2.4.16",
    "watchify": "^2.4.0"
  },
  "homepage": "https://github.com/eliias/jules",
  "keywords": [
    "Gamejam",
    "Game",
    "HTML5",
    "JavaScript"
  ],
  "license": "proprietary",
  "main": "scripts/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/eliias/jules.git"
  },
  "scripts": {
    "build": "browserify scripts/main.js | uglifyjs > game.bundle.js",
    "watch": "watchify scripts/main.js -o game.bundle.js"
  }
}
